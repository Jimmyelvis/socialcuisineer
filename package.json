  "scripts": {
    "watch:sass": "sass assets/sass/main.scss assets/css/style.css --watch",
    "start": "npm-run-all --parallel watch:sass dev sync",
    "compile:sass": "sass assets/sass/main.scss assets/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.comp.css -o css/style.prefix.css",
    "compress:css": "sass css/style.prefix.css css/style.css --style=compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "build": "webpack --mode production --progress",
    "sync": "browser-sync start --proxy http://localhost/Social-Cuisiener-2025 --port 3065 --ui-port 3075 --files '**/*.css' '**/*.js' --no-inject-changes",
    "dev": "webpack -w",
    "hot": "webpack serve --mode development --open",
    "hot-dev": "npm-run-all --parallel watch:sass hot sync",
    "hot-dev-wp": "npm-run-all --parallel watch:sass hot sync dev"
  },
